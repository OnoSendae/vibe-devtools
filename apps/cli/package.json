        "agentic",
        "cursor",
        "commands",
        "package-manager"
    ],
    "author": "Vibes Team",
    "license": "MIT",
    "dependencies": {
        "commander": "^12.0.0",
        "prompts": "^2.4.2",
        "chalk": "^5.3.0",
        "ora": "^8.0.1",
        "cli-table3": "^0.6.3"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/prompts": "^2.4.9",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0",
        "eslint": "^8.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}