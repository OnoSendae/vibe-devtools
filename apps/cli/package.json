{
    "name": "vibe-devtools",
    "version": "0.6.0",
    "description": "CLI tool to install and manage vibes (agentic command packages)",
    "type": "module",
    "main": "dist/index.js",
    "bin": {
        "vibes": "./dist/index.js"
    },
    "files": [
        "dist/"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "start": "node dist/index.js",
        "test": "vitest",
        "lint": "eslint src/"
